{
  "name": "mdsproject",
  "version": "1.0.0",
  "description": "Basic interface to show images and allow to evaluate if they are synthetic or real",
  "main": "frontend/index.html",
  "scripts": {
    "start": "npm run clean && npm run build && node dist/app.js",
    "dev": "nodemon lib/app.ts",
    "build": "tsc -p .",
    "clean": "rm -r dist/* || true",
    "test": "echo \"Error: no test specified\" && exit 1",
    "sass": "node-sass frontend/scss/ -o frontend/css/",
    "watch-sass": "rerun-script"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/emanuelebeozzo/projectMDS.git"
  },
  "author": "Beozzo Emanuele, Xompero Leonardo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/emanuelebeozzo/projectMDS/issues"
  },
  "homepage": "https://github.com/emanuelebeozzo/projectMDS#readme",
  "watches": {
    "sass": "frontend/scss/**"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.10",
    "nodemon": "^2.0.15",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2",
    "node-sass": "^6.0.1",
    "rerun-script": "^0.6.0"
  },
  "dependencies": {
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "mongoose": "^6.0.14",
    "node-sass": "^6.0.1",
    "util": "^0.12.4"
  }
}
